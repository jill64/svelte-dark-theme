{
  "name": "@jill64/svelte-dark-theme",
  "version": "1.0.22",
  "description": "Dark theme manager for Svelte ",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/svelte-dark-theme/issues",
  "homepage": "https://github.com/jill64/svelte-dark-theme#readme",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/svelte-dark-theme.git"
  },
  "keywords": [
    "dark-mode",
    "ssr",
    "sveltekit"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && pnpm package",
    "preview": "pnpm build && vite preview",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "format": "npx prettier --write . --plugin prettier-plugin-svelte .",
    "lint": "npx prettier --check . --plugin prettier-plugin-svelte . && npx eslint"
  },
  "peerDependencies": {
    "svelte": "4.2.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "2.0.3",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-svelte": "2.34.0",
    "prettier-plugin-svelte": "3.0.3",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "4.4.11"
  },
  "dependencies": {
    "@jill64/attempt": "1.0.6",
    "@jill64/svelte-device-theme": "1.0.4",
    "@jill64/typed-storage": "0.3.15",
    "@sveltejs/kit": "1.25.2",
    "@types/cookie": "0.5.2",
    "cookie": "0.5.0",
    "svelte": "4.2.1",
    "typescanner": "0.5.3"
  }
}
